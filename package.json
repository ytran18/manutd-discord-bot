{
  "name": "mu-bot",
  "version": "1.0.0",
  "description": "Discord bot for Manchester United updates",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node dist/bot.js",
    "dev": "nodemon",
    "build": "tsc",
    "watch": "tsc -w",
    "prod": "npm run build && pm2 start ecosystem.config.js",
    "prod:stop": "pm2 stop mu-bot",
    "prod:restart": "pm2 restart mu-bot",
    "prod:logs": "pm2 logs mu-bot"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "node-cron": "^3.0.3",
    "pm2": "^5.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
